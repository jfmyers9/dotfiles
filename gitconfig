[alias]
	ci = commit
	co = checkout
	su = submodule update --init --recursive
[push]
	recurseSubmodule=check
[url "git@github.com:"]
	pushInsteadOf = https://github.com/
[user]
	email = jmyers@brex.com
	name = James Myers
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
